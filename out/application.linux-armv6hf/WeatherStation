#!/bin/sh

APPDIR=$(readlink -f "$0")
APPDIR=$(dirname "$APPDIR")
java -Djna.nosys=true -Djava.library.path="$APPDIR:$APPDIR/lib" -cp "$APPDIR:$APPDIR/lib/gson-2.8.6.jar:$APPDIR/lib/OkHttpClient.jar:$APPDIR/lib/WeatherStation.jar:$APPDIR/lib/core.jar:$APPDIR/lib/jogl-all.jar:$APPDIR/lib/gluegen-rt.jar:$APPDIR/lib/jogl-all-natives-linux-armv6hf.jar:$APPDIR/lib/gluegen-rt-natives-linux-armv6hf.jar:$APPDIR/lib/commons-codec-1.4.jar:$APPDIR/lib/commons-logging-1.1.1.jar:$APPDIR/lib/httpclient-4.1.2.jar:$APPDIR/lib/httpclient-cache-4.1.2.jar:$APPDIR/lib/httpcore-4.1.2.jar:$APPDIR/lib/httpmime-4.1.2.jar:$APPDIR/lib/httprequests_processing.jar" WeatherStation "$@"
